<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/seek_controller_pressed"/>
            <corners android:radius="@dimen/seek_controller_big_radius"/>
            <size android:width="@dimen/seek_controller_big_diameter" android:height="@dimen/seek_controller_big_diameter"/>
        </shape>
    </item>
    <item android:left="@dimen/seek_controller_padding"
        android:top="@dimen/seek_controller_padding"
        android:right="@dimen/seek_controller_padding"
        android:bottom="@dimen/seek_controller_padding">
        <shape android:shape="rectangle">
            <solid android:color="@color/seek_controller"/>
            <corners android:radius="@dimen/seek_controller_radius"/>
            <size android:width="@dimen/seek_controller_diameter" android:height="@dimen/seek_controller_diameter"/>
        </shape>
    </item>
</layer-list>
